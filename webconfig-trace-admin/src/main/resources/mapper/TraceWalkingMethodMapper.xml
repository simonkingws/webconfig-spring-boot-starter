<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.simonkingws.webconfig.trace.admin.mapper.TraceWalkingMethodMapper">
    <select id="statServerInvokeCount" resultType="com.simonkingws.webconfig.trace.admin.vo.ServerInvokeVO">
        SELECT
            m.provider_server_name as serverName,
            COUNT(*) as invokeCount
        FROM trace_walking_method m
        GROUP BY m.provider_server_name
    </select>
</mapper>
